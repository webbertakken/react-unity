{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "commonjs": true,
    "node": true,
    "es6": true,
    "jest": true,
    "cypress/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "react-app",
    "airbnb",
    "airbnb/hooks",
    "plugin:unicorn/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "react",
    "jsx-a11y",
    "import",
    "prettier",
    "cypress",
    "flowtype",
    "unicorn"
  ],
  "rules": {
    "prettier/prettier": "error",
    "jsx-a11y/href-no-hash": 0,
    "jsx-a11y/anchor-is-valid": ["error", {"components": ["Link"], "specialLink": ["to"]}],
    "react/jsx-filename-extension": [1, {"extensions": [".js"]}],
    "no-console": 0
  }
}
